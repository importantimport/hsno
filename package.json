{
  "name": "hsno",
  "description": "!mportantImport's Next-Gen Blog Starter.",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.static": "vite build --ssr src/entry.static.tsx",
    "build.types": "tsc --incremental --noEmit",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "ssg": "node server/entry.static",
    "start": "vite --open --mode ssr",
    "qwik": "qwik",
    "partytown": "partytown copylib public/~partytown"
  },
  "devDependencies": {
    "@builder.io/partytown": "^0.7.1",
    "@builder.io/qwik": "^0.12.0",
    "@builder.io/qwik-city": "^0.0.118",
    "@types/eslint": "^8.4.8",
    "@types/node": "latest",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "eslint": "^8.26.0",
    "eslint-plugin-qwik": "^0.12.0",
    "fff-flavored-frontmatter": "0.4.0-alpha.0",
    "globals": "^13.17.0",
    "prettier": "^2.7.1",
    "prettier-config-standard": "^5.0.0",
    "qwik-speak": "^0.1.2",
    "typescript": "^4.8.4",
    "vite": "^3.2.0",
    "vite-plugin-pwa": "^0.13.1",
    "vite-tsconfig-paths": "^3.5.2",
    "workbox-precaching": "^6.5.4"
  }
}
